//--=============== test



//! Structure type containing variables to store in NVRAM using a specific
//! memory map.
typedef const struct {
  uint8_t  var8;
  uint16_t var16;
  uint8_t  var8_3[3];
  uint32_t var32;
} nvram_data_t;

